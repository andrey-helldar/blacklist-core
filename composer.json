{
    "name": "andrey-helldar/blacklist-core",
    "description": "Core for blacklist server and client",
    "type": "library",
    "license": "MIT",
    "keywords": ["spam", "blacklist", "blacklist", "antispam", "anti-spam", "laravel", "blacklist", "server", "client", "core"],
    "support": {
        "issues": "https://github.com/andrey-helldar/blacklist-core/issues",
        "source": "https://github.com/andrey-helldar/blacklist-core"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\BlacklistCore\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
